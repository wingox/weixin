/* projects/index/welcome.wxss */
.container{
    display: flex;
    flex-direction: column;
    align-items: center;
}
.container .user-image{
    height: 200rpx;
    width: 200rpx;
    border-radius: 50%;
    margin-top: 120px;
}
.container .user-name{
    font-size: 32rpx;
    font-weight: bold;
    margin-top: 120rpx;
}
.container .moveto-container {
   font-size: 25rpx;
   border:1px solid #000;
   height: 80rpx;
   text-align:center;
   border-radius: 5px;
   margin-top:200rpx;
}
.container .moveto-container .moveto{
    line-height:80rpx;
}